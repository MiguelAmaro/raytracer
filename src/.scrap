  World->SphereOnePastLast       = World->Spheres      +WORLD_STACK_MAXCOUNT;
  World->SphereMovingOnePastLast = World->SpheresMoving+WORLD_STACK_MAXCOUNT;
  World->PlaneOnePastLast        = World->Planes       +WORLD_STACK_MAXCOUNT;
